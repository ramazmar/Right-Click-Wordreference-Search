{
"name":"Right-Click Wordreference Search",
"description":"Use the right-click menu to translate for selected text in Wordreference",
"version":"0.5.0.1",
"manifest_version":2,
"web_accessible_resources": [
    "fonts/*",
    "images/*"
],
"options_page": "settings.html",
"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
"permissions": [ "tabs", "contextMenus", "http://*/*", "https://*/*","alarms" ],
"background": { 
      "scripts": ["background.js"]
    },
"content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      "js": ["jquery-3.0.0.min.js","scripts.js"]
    }
  ]
}
